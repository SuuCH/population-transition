{
  "name": "population-trasition",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fmt": "npx sort-package-json package.json && next lint --fix && stylelint --fix \"**/*.{css,scss}\" && prettier --write --ignore-unknown \"**/*\"",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint && yarn lint:tsc && yarn lint:unused",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check --ignore-unknown \"**/*\"",
    "lint:stylelint": "stylelint \"**/*.{css,scss}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:unused": "next-unused",
    "start": "next start"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "next-unused": "^0.0.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.5.4"
  }
}
