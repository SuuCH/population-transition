{
  "name": "population-trasition",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook",
    "dev": "next dev",
    "fmt": "npx sort-package-json package.json && next lint --fix && stylelint --fix \"**/*.{css,scss}\" && prettier --write --ignore-unknown \"**/*\"",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint && yarn lint:tsc && yarn lint:unused",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check --ignore-unknown \"**/*\"",
    "lint:stylelint": "stylelint \"**/*.{css,scss}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:unused": "next-unused",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "start-storybook -p 6006"
  },
  "lint-staged": {
    "**/*": "yarn lint:prettier",
    "**.{ts,tsx}": "yarn run eslint --cache --ext",
    "*.{css,scss}": "yarn lint:stylelint"
  },
  "eslintIgnore": [
    ".eslintrc.js",
    "next.config.js",
    "storybook-static/**"
  ],
  "dependencies": {
    "highcharts": "^9.3.2",
    "highcharts-react-official": "^3.1.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "css-loader": "^6.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-ja": "^0.0.2",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "next-unused": "^0.0.6",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0",
    "storybook-addon-designs": "^6.2.0",
    "storybook-addon-next-router": "^3.1.1",
    "storybook-addon-outline": "^1.4.2",
    "storybook-addon-pseudo-states": "^1.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.5.4"
  }
}
